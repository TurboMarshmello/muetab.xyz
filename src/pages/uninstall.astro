---
import Base from "../layouts/Base.astro";
---

<Base frontmatter={{ title: "Thank you for using Mue." }}>
	<main class="py-8 lg:py-16">
		<iframe
			data-tally-src="https://tally.so/embed/nPpGd1?transparentBackground=1&dynamicHeight=1"
			loading="lazy"
			width="100%"
			height="864"
			frameborder="0"
			marginheight="0"
			marginwidth="0"></iframe>
		<script is:inline>
			var d = document,
				w = "https://tally.so/widgets/embed.js",
				v = function () {
					"undefined" != typeof Tally
						? Tally.loadEmbeds()
						: d
								.querySelectorAll(
									"iframe[data-tally-src]:not([src])"
								)
								.forEach(function (e) {
									e.src = e.dataset.tallySrc;
								});
				};
			if ("undefined" != typeof Tally) v();
			else if (d.querySelector('script[src="' + w + '"]') == null) {
				var s = d.createElement("script");
				(s.src = w),
					(s.onload = v),
					(s.onerror = v),
					d.body.appendChild(s);
			}
		</script>
	</main>
</Base>
